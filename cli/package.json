{
  "name": "psychic-homily-cli",
  "version": "1.0.0",
  "description": "Admin CLI for Psychic Homily show export/import",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ph-admin": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.tsx",
    "build": "tsc",
    "build:binary": "bun build --compile --outfile ph-admin src/index.tsx",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "open": "^11.0.0",
    "react": "^18.3.1",
    "react-devtools-core": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.12",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
